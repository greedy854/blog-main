{
    "builds": [
        {
            "src": "App.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.)",
            "dest": "/App.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "rewrites": [
        {
            "source": "/(.)",
            "destination": "/"
        }
    ]
}